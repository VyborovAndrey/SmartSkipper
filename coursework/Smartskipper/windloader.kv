<WindLoader>:
    id: windloader
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        FileChooserIconView:
            id: filechooser
            multiselect: True
			path: './'
            filters: ['*.json']
        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"
                on_release: windloader.cancel()

            Button:
                text: "Load"
                on_release: windloader.load(filechooser.selection), windloader.cancel()